{
  "ucf101": {
    "path": "data/ucf101/videos",
    "ext": ".avi",
    "class_index": "data/ucf101/annotations/classInd.txt",
    "n_videos": 13320,
    "n_classes": 101
  },
  "hmdb51": {
    "path": "data/hmdb51/videos"
  },
  "xgtf": {
    "path": "data/ucf101/xgtf",
    "mask": {
      "path": "data/ucf101/xgtf-mask",
      "action_only": false,
      "ext": ".png"
    }
  },
  "mat": {
    "path": "data/hmdb51/mat-mask"
  },
  "relevancy": {
    "output": "data/relevancy",
    "detector": {
      "name": "unidet",
      "classnames": "UniDet/classnames.json"
    },
    "json": "data/relevancy/unidet-ids/all-mpnet-base-v2/0.5.json",
    "avail_methods": [
      "all-mpnet-base-v2",
      "multi-qa-mpnet-base-dot-v1",
      "all-distilroberta-v1",
      "all-MiniLM-L12-v2",
      "multi-qa-distilbert-cos-v1",
      "all-MiniLM-L6-v2",
      "multi-qa-MiniLM-L6-cos-v1",
      "paraphrase-multilingual-mpnet-base-v2",
      "paraphrase-albert-small-v2",
      "paraphrase-multilingual-MiniLM-L12-v2"
    ]
  },
  "e2fgvi": {
    "ucf101": {
      "path": "../data/ucf101/videos"
    },
    "model": "model.e2fgvi_hq",
    "mask": "../data/ucf101/xgtf-mask",
    "checkpoint": "release_model/E2FGVI-HQ-CVPR22.pth",
    "output": "../data/ucf101/scene-xgtf",
    "max_video_len": 400,
    "video": {
      "reader": "opencv",
      "writer": "moviepy"
    }
  },
  "unidet": {
    "detect": {
      "dataset": {
        "path": "../data/ucf101/videos",
        "ext": ".avi"
      },
      "config": "configs/Unified_learned_OCIM_RS200_6x+2x.yaml",
      "checkpoint": "models/Unified_learned_OCIM_RS200_6x+2x.pth",
      "output": {
        "json": "../data/ucf101/unidet-json",
        "video": {
          "path": "../data/ucf101/unidet",
          "writer": "moviepy",
          "generate": true
        }
      },
      "confidence": 0.5,
      "parallel": false
    },
    "select": {
      "mode": "intercutmix",
      "dataset": {
        "path": "data/ucf101/videos",
        "ext": ".avi"
      },
      "confidence": 0.5,
      "common_objects": ["Person", "Man", "Woman"],
      "json": "../data/ucf101/unidet-json",
      "video_reader": "opencv",
      "output": {
        "video": {
          "generate": true,
          "writer": "moviepy",
          "path": "data/ucf101/unidet-inter"
        },
        "mask": {
          "generate": false,
          "path": "data/ucf101/unidet-inter-mask",
          "ext": ".png"
        },
        "repp": {
          "enabled": false,
          "path": "data/ucf101/unidet-inter-dump"
        }
      }
    }
  },
  "repp": {
    "input": {
      "path": "data/ucf101/repp-inter-json"
    },
    "video_reader": "opencv",
    "dataset": "ucf101",
    "output": {
      "video": {
        "path": "data/ucf101/repp-inter",
        "writer": "moviepy"
      }
    }
  },
  "mix": {
    "multiplication": 10,
    "video": {
      "reader": "opencv",
      "writer": "moviepy",
      "blacklist": [
        "v_PommelHorse_g05_c01",
        "v_PommelHorse_g05_c02",
        "v_PommelHorse_g05_c03",
        "v_PommelHorse_g05_c04"
      ]
    },
    "action": {
      "whitelist": null,
      "blacklist": null
    },
    "multithread": false,
    "dataset": {
      "path": "/nas.dbms/randy/datasets/ucf24",
      "ext": ".avi",
      "n_videos": 3207
    },
    "scene": {
      "list": "data/ucf101/scene-xgtf.json",
      "path": "data/ucf101/scene-xgtf",
      "ext": ".mp4"
    },
    "mask": {
      "path": "data/ucf101/xgtf-mask",
      "ext": ".png"
    },
    "output": {
      "path": "data/ucf101/ucf24-mix",
      "ext": ".mp4"
    }
  }
}
