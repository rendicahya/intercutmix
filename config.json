{
  "ucf101": {
    "path": "data/ucf101/videos",
    "ext": ".avi",
    "class_index": "data/ucf101/annotations/classInd.txt",
    "n_videos": 13320,
    "n_classes": 101
  },
  "hmdb51": {
    "path": "data/hmdb51/videos"
  },
  "xgtf": {
    "path": "data/ucf101/xgtf/files",
    "mask": {
      "path": "data/ucf101/xgtf/mask",
      "action_only": false,
      "ext": ".png"
    }
  },
  "mat": {
    "path": "data/hmdb51/mat-mask"
  },
  "relevancy": {
    "output": "data/relevancy",
    "detector": {
      "name": "UniDet",
      "classnames": "UniDet/classnames.json"
    },
    "json": "data/relevancy/UniDet/ids/all-mpnet-base-v2/0.5.json",
    "avail_methods": [
      "all-mpnet-base-v2",
      "multi-qa-mpnet-base-dot-v1",
      "all-distilroberta-v1",
      "all-MiniLM-L12-v2",
      "multi-qa-distilbert-cos-v1",
      "all-MiniLM-L6-v2",
      "multi-qa-MiniLM-L6-cos-v1",
      "paraphrase-multilingual-mpnet-base-v2",
      "paraphrase-albert-small-v2",
      "paraphrase-multilingual-MiniLM-L12-v2"
    ]
  },
  "e2fgvi": {
    "ucf101": {
      "path": "../data/ucf101/videos"
    },
    "model": "model.e2fgvi_hq",
    "mask": "../data/ucf101/xgtf/mask",
    "checkpoint": "release_model/E2FGVI-HQ-CVPR22.pth",
    "output": "../data/ucf101/xgtf/scene",
    "max_video_len": 400,
    "video": {
      "reader": "opencv",
      "writer": "moviepy"
    }
  },
  "unidet": {
    "detect": {
      "dataset": {
        "path": "data/ucf101/videos",
        "ext": ".avi"
      },
      "config": "configs/Unified_learned_OCIM_RS200_6x+2x.yaml",
      "checkpoint": "models/Unified_learned_OCIM_RS200_6x+2x.pth",
      "output": {
        "json": "data/ucf101/UniDet/detect/json",
        "video": {
          "generate": false,
          "path": "data/ucf101/UniDet/detect/videos",
          "writer": "moviepy"
        }
      },
      "confidence": 0.5,
      "parallel": false
    },
    "select": {
      "mode": "intercutmix",
      "dataset": {
        "path": "data/ucf101/videos",
        "ext": ".avi"
      },
      "confidence": 0.5,
      "common_objects": ["Person", "Man", "Woman"],
      "json": "data/ucf101/UniDet/json",
      "video_reader": "opencv",
      "output": {
        "video": {
          "generate": false,
          "writer": "moviepy",
          "path": "data/ucf101/UniDet/select/inter/videos"
        },
        "mask": {
          "generate": false,
          "path": "data/ucf101/UniDet/select/inter/mask",
          "ext": ".png"
        },
        "dump": {
          "enabled": true,
          "path": "data/ucf101/UniDet/select/inter/dump"
        }
      }
    }
  },
  "repp": {
    "configuration": "REPP_cfg/fgfa_repp_cfg.json",
    "video": {
      "path": "data/ucf101/videos",
      "reader": "opencv"
    },
    "output": {
      "video": {
        "generate": true,
        "path": "data/ucf101/UniDet/REPP/inter/videos",
        "writer": "moviepy",
        "ext": ".mp4"
      },
      "mask": {
        "path": "data/ucf101/UniDet/REPP/inter/mask"
      }
    }
  },
  "mix": {
    "multiplication": 10,
    "video": {
      "reader": "opencv",
      "writer": "moviepy",
      "blacklist": [
        "v_PommelHorse_g05_c01",
        "v_PommelHorse_g05_c02",
        "v_PommelHorse_g05_c03",
        "v_PommelHorse_g05_c04"
      ]
    },
    "action": {
      "whitelist": null,
      "blacklist": null
    },
    "dataset": {
      "path": "data/ucf101/videos",
      "ext": ".avi"
    },
    "scene": {
      "list": "data/ucf101/xgtf/scene-list.json",
      "path": "data/ucf101/xgtf/scene",
      "ext": ".mp4"
    },
    "mask": {
      "path": "data/ucf101/xgtf-mask"
    },
    "output": {
      "path": "data/ucf101/intercutmix",
      "ext": ".mp4"
    }
  }
}
