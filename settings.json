{
  "active": {
    "dataset": "hmdb51",
    "mode": "actorcutmix",
    "detector": "UniDet",
    "object_selection": true,
    "use_REPP": true,
    "random_seed": 0,
    "smooth_mask": {
      "enabled": false,
      "gaussian_size": 35
    },
    "video": {
      "reader": "opencv",
      "writer": "moviepy"
    }
  },
  "ucf101": {
    "path": "data/ucf101/videos",
    "ext": ".avi",
    "class_index": "data/ucf101/annotations/classInd.txt",
    "n_videos": 13320,
    "n_classes": 101,
    "scene": {
      "path": "data/ucf101/UniDet/select/intercutmix/REPP/scene",
      "ext": ".mp4"
    },
    "annotation": {
      "path": "data/ucf101/xgtf/files",
      "mask": {
        "path": "data/ucf101/xgtf/mask",
        "action_only": false
      }
    }
  },
  "hmdb51": {
    "path": "data/hmdb51/videos",
    "ext": ".avi",
    "split": 1,
    "n_videos": 6766,
    "n_classes": 51,
    "frames": "data/hmdb51/frames",
    "scene": {
      "path": "data/hmdb51/UniDet/select/intercutmix/REPP/scene",
      "ext": ".mp4"
    },
    "annotation": {
      "path": "data/hmdb51/mat/files",
      "mask": {
        "box": true,
        "path": "data/hmdb51/mat/mask"
      }
    }
  },
  "kinetics100": {
    "path": "data/kinetics100_p10",
    "ext": ".mp4",
    "n_classes": 100,
    "partition": "10p",
    "make": "symlink"
  },
  "kinetics400": {
    "path": "../../datasets/kinetics400",
    "ext": ".mp4",
    "filelist": "k400-filelist.json",
    "replacements": {
      "list": "k400-replacements.json",
      "dir": "replacement/replacement_for_corrupted_k400"
    }
  },
  "relevancy": {
    "output": "data/relevancy",
    "detector": {
      "UniDet": {
        "classnames": "UniDet/classnames.json"
      }
    },
    "active": {
      "method": "all-mpnet-base-v2",
      "threshold": 0.5
    },
    "avail_methods": [
      "all-mpnet-base-v2",
      "multi-qa-mpnet-base-dot-v1",
      "all-distilroberta-v1",
      "all-MiniLM-L12-v2",
      "multi-qa-distilbert-cos-v1",
      "all-MiniLM-L6-v2",
      "multi-qa-MiniLM-L6-cos-v1",
      "paraphrase-multilingual-mpnet-base-v2",
      "paraphrase-albert-small-v2",
      "paraphrase-multilingual-MiniLM-L12-v2"
    ]
  },
  "e2fgvi": {
    "input": {
      "ucf101": {
        "type": "videos",
        "video": {
          "max_len": 400
        }
      },
      "hmdb51": {
        "type": "frames",
        "video": {
          "max_len": 400
        }
      }
    },
    "model": "model.e2fgvi_hq",
    "checkpoint": "release_model/E2FGVI-HQ-CVPR22.pth"
  },
  "unidet": {
    "detect": {
      "config": "configs/Unified_learned_OCIM_RS200_6x+2x.yaml",
      "checkpoint": "models/Unified_learned_OCIM_RS200_6x+2x.pth",
      "confidence": 0.5,
      "parallel": false,
      "generate_videos": false
    },
    "select": {
      "confidence": 0.5,
      "common_objects": ["Person", "Man", "Woman"],
      "output": {
        "mask": true,
        "dump": true,
        "video": false
      }
    }
  },
  "repp": {
    "configuration": "REPP_cfg/fgfa_repp_cfg.json",
    "output": {
      "video": {
        "generate": false,
        "ext": ".mp4"
      }
    }
  },
  "cutmix": {
    "multiplication": 10,
    "from_dump": true,
    "output": {
      "ext": ".mp4"
    }
  }
}
